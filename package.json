{
  "name": "react-leonardo-ui",
  "version": "0.2.0",
  "description": "React implementation of Leonardo UI",
  "author": "Rickard Svensson <svensson.rt@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ricsv/react-leonardo-ui.git"
  },
  "files": [
    "/dist"
  ],
  "module": "src/react-leonardo-ui.js",
  "main": "dist/react-leonardo-ui.js",
  "scripts": {
    "build": "rollup -c && rollup -c --environment BUILD:production",
    "jest": "jest",
    "lint": "eslint .",
    "start": "rollup -c ./rollup.config.docs.js -w",
    "test": "npm run lint && npm run jest --  --coverage"
  },
  "peerDependencies": {
    "leonardo-ui": ">=1.0.2",
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-jest": "^18.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "jest": "^18.1.0",
    "leonardo-ui": ">=1.0.2",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-test-renderer": "^15.4.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-serve": "^0.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  }
}
